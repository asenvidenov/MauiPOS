<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="MauiPOS.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:MauiPOS.Views"
    Shell.FlyoutBehavior="Disabled"
    Title="MauiPOS">

    <ShellContent
        Title="LogIn"
        ContentTemplate="{DataTemplate local:CashLogin}"
        Route="CashLogin" />
    <ShellContent
    Title="OrderDetails"
    ContentTemplate="{DataTemplate local:MAUIOrder}"
    Route="MAUIOrder" />

</Shell>
